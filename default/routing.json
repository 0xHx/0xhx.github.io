{
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "balancers": [{
            "tag": "blc-vls",
            "selector": ["vls", "vms", "goo"]
        }, {
            "tag": "blc-qic",
            "selector": ["qic"]
        }, {
            "tag": "blc-tor",
            "selector": ["tor"]
        }, {
            "tag": "blc-opc",
            "selector": ["opc"]
        }],
        "rules": [{
            "_": "一些域名绑定，直通",
            "type": "field",
            "domain": [ "mydomaincn" ],
            "outboundTag": "direct"
        }, {
            "_": "爬虫通道绑定",
            "type": "field",
            "inboundTag": ["opc", "opch"],
            "balancerTag": "blc-opc"
        }, {
            "type": "field",
            "domain": ["frp.io"],
            "outboundTag": "frp"
        }, {
            "_": "tor 通道绑定",
            "type": "field",
            "inboundTag": ["tor", "torh"],
            "balancerTag": "blc-tor"
        }, {
            "type": "field",
            "inboundTag": ["socks", "http","qic","qich"],
            "ip": ["geoip:private", "geoip:cn"],
            "outboundTag": "direct"
        }, {
            "_": "qic 通道绑定",
            "type": "field",
            "inboundTag": ["qic","qich", "dns"],
            "balancerTag": "blc-qic"
        }, {
            "_": "默认 通道绑定",
            "type": "field",
            "inboundTag": ["socks", "http"],
            "balancerTag": "blc-vls"
        }]
    }
}
